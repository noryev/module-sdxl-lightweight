{
  "machine": {
    "gpu": 1,
    "cpu": 1000,
    "ram": 1000
  },
  "job": {
    "APIVersion": "V1beta1",
    "Metadata": {
      "CreatedAt": "0001-01-01T00:00:00Z",
      "Requester": {}
    },
    "Spec": {
      "Deal": {
        "Concurrency": 1
      },
      "Docker": {
        "Entrypoint": null,
        "EnvironmentVariables": [
          {{ if .Prompt }}"PROMPT={{ .Prompt }}",{{ else }}"PROMPT=A lilypad floating on a galaxy of stars",{{ end }}
          {{ if .Seed }}"SEED={{ .Seed }}",{{ else }}"SEED=42",{{ end }}
          {{ if .Steps }}"STEPS={{ .Steps }}",{{ else }}"STEPS=30",{{ end }}
          {{ if .Sampler }}"SAMPLER={{ .Sampler }}",{{ end }}
          {{ if .Scheduler }}"SCHEDULER={{ .Scheduler }}",{{ end }}
          {{ if .Size }}"SIZE={{ .Size }}",{{ end }}
          {{ if .Batching }}"BATCHING={{ .Batching }}",{{ end }}
          "HF_HUB_OFFLINE=1",
          "OUTPUT_DIR=/outputs/"
        ],
        "Image": "noryev/sdxl-lightweight:latest"
      },
      "Engine": "Docker",
      "Network": {
        "Type": "None"
      },
      "Outputs": [
        {
          "Name": "outputs",
          "Path": "/outputs"
        }
      ],
      "PublisherSpec": {
        "Type": "ipfs"
      },
      "Resources": {
        "GPU": "1"
      },
      "Timeout": 1800,
      "Wasm": {
        "EntryModule": {}
      }
    }
  }
}