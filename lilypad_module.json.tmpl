{
  "machine": {
    "gpu": 1,
    "cpu": 1000,
    "ram": 1000
  },
  "job": {
    "APIVersion": "V1beta1",
    "Spec": {
      "Deal": {"Concurrency": 1},
      "Docker": {
        "Entrypoint": [
          "python", "/app/sdxl_script.py",
          "--prompt", "{{ .PROMPT }}",
          "--seed", "{{ .SEED }}",
          "--steps", "{{ .STEPS }}",
          "--height", "{{ .HEIGHT }}",
          "--width", "{{ .WIDTH }}",
          "--output_dir", "/outputs/"
        ],
        "Image": "noryev/sdxl-lightweight:latest"
      },
      "Engine": "Docker",
      "Network": {"Type": "None"},
      "Outputs": [{"Name": "outputs", "Path": "/outputs"}],
      "PublisherSpec": {"Type": "ipfs"},
      "Resources": {"GPU": "1"},
      "Timeout": 1800
    }
  }
}